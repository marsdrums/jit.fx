einput = 1;\n\t\tslab.jit_gl_texture(kernelTex.name);\n\t}\n}\n\nfunction jit_gl_texture(inname){\n\n\tif(inlet == 1){\n\t\tslab.activeinput = 1;\n\t\tslab.jit_gl_texture(inname);\n\n\t} else {\n\n\t\tslab.activeinput = 0;\n\t\tslab.jit_gl_texture(inname);\n\t\tslab.draw();\n\t}\n\n\toutlet(0, \"jit_gl_texture\", slab.out_name);\n}",
